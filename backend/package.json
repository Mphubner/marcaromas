    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.4.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sharp": "^0.32.4",
    "slugify": "^1.6.6",
    "stripe": "^14.9.0",
    "winston": "^3.18.3",
